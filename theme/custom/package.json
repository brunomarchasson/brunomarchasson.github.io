{
  "name": "jsonresume-theme-local",
  "version": "2.1.2",
  "license": "MIT",
  "main": "dist/ssr/index.js",
  "type": "module",
  "bin": {
    "jsonresume-theme-local": "./bin/generate.js"
  },
  "exports": {
    "./en": "./dist/en/index.js",
    ".": "./dist/ssr/index.js"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^12.8.0",
    "@lucide/lab": "^0.1.2",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "clsx": "^2.1.1",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.511.0",
    "luxon": "^3.6.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.1",
    "react-icons": "^5.5.0",
    "simple-icons": "^14.14.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "eslint": "^9.27.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "scripts": {
    "dev": "vite dev --config vite.config.ts",
    "build:client": "vite build --config vite.config.ts --outDir dist/client",
    "build:ssr": "set VITE_SSR_BUILD=1 && vite build --config vite.config.ts --outDir dist/ssr",
    "postbuild:copy-assets": "npx shx cp -r dist/client/assets dist/ssr/assets",
    "build": "pnpm run build:client && pnpm run build:ssr && pnpm run postbuild:copy-assets",
    "preview": "vite preview",
    "lint": "pnpm eslint .",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "clean": "rimraf dist storybook-static",
    "prepare": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md",
    "style.css"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}